package com.selenium;

public class Trainee {
	public static void main(String[] args) {
		try {
			badMethod();
			System.out.print("A");
		} catch (Exception ex) {
			System.out.print("C");
		} finally {
			System.out.print("B");
		}
		System.out.print("D");
	}
	public static void badMethod() {
		try {
		throw new Exception();
		}
		catch (Exception e){
			System.out.println(e.toString());
		}
	}
}

